{
  "name": "@flexkit/studio",
  "version": "0.0.6",
  "description": "Flexkit Studio - Build custom internal tools like PIMs, CRMs, and ERPs with a flexible React UI and a graph-backed API",
  "keywords": [
    "flexkit",
    "studio",
    "react",
    "nextjs",
    "node",
    "pim",
    "crm",
    "erp",
    "graphql"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexkit-io/flexkit",
    "directory": "packages/flexkit-studio"
  },
  "exports": {
    ".": "./dist/index.js",
    "./nextjs": "./dist/nextjs.js",
    "./astro": "./dist/astro.js",
    "./hono": "./dist/hono.js",
    "./express": "./dist/express.js",
    "./tanstack-start": "./dist/tanstack-start.js",
    "./ui": "./dist/ui.js",
    "./tailwind.config": "./tailwind.config.ts",
    "./styles.css": "./dist/index.css"
  },
  "typesVersions": {
    "*": {
      "nextjs": [
        "./dist/nextjs.d.ts"
      ],
      "astro": [
        "./dist/astro.d.ts"
      ],
      "hono": [
        "./dist/hono.d.ts"
      ],
      "express": [
        "./dist/express.d.ts"
      ],
      "tanstack-start": [
        "./dist/tanstack-start.d.ts"
      ]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o dist/index.css && tsup --config tsup.config.ts && tsup src/ssr.tsx src/nextjs.ts src/astro.ts src/hono.ts src/express.ts src/tanstack-start.ts --format esm --dts --external react --external react-dom --no-config",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch & tsup --config tsup.config.ts --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "ui:add": "pnpm dlx shadcn@latest add"
  },
