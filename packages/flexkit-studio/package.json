{
  "name": "@flexkit/studio",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./data-grid": "./dist/data-grid/index.js",
    "./ssr": "./dist/ssr.js",
    "./tailwind.config": "./tailwind.config.ts",
    "./styles.css": "./dist/index.css"
  },
  "typesVersions": {
    "*": {
      "ssr": [
        "./dist/ssr.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx src/ssr.tsx src/data-grid/index.tsx --format esm,cjs --dts --external react & tailwindcss -i ./src/styles.css -o dist/index.css",
    "dev": "tsup src/index.tsx src/ssr.tsx src/data-grid/index.tsx --format esm,cjs --watch --dts --external react & tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "ui:add": "pnpm dlx shadcn-ui@latest add"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^20.5.2",
    "@types/ramda": "^0.29.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.6",
    "autoprefixer": "^10.4.16",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "3.4.10",
    "tailwindcss-animate": "1.0.7",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@hookform/resolvers": "^3.3.2",
    "@lexical/code": "^0.12.2",
    "@lexical/html": "^0.12.2",
    "@lexical/link": "^0.12.2",
    "@lexical/list": "^0.12.2",
    "@lexical/markdown": "^0.12.2",
    "@lexical/react": "^0.12.2",
    "@lexical/rich-text": "^0.12.2",
    "@lexical/selection": "^0.12.2",
    "@lexical/table": "^0.12.2",
    "@lexical/utils": "^0.12.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
