{
  "name": "@flexkit/studio",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./data-grid": "./dist/data-grid/index.js",
    "./ssr": "./dist/ssr.js",
    "./ui": "./dist/ui.js",
    "./tailwind.config": "./tailwind.config.ts",
    "./styles.css": "./dist/index.css"
  },
  "typesVersions": {
    "*": {
      "ssr": [
        "./dist/ssr.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx src/ssr.tsx src/ui.tsx src/data-grid/index.tsx --format esm --dts --external react && tailwindcss -i ./src/styles.css -o dist/index.css",
    "dev": "tsup src/index.tsx src/ssr.tsx src/ui.tsx src/data-grid/index.tsx --format esm --watch --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "ui:add": "pnpm dlx shadcn@latest add"
  },
