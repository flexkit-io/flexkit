{
  "name": "@flexkit/studio",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./ssr": "./dist/ssr.js",
    "./plugins": "./dist/plugins.js",
    "./ui": "./dist/ui.js",
    "./data-grid": "./dist/data-grid.js",
    "./styles.css": "./src/styles.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**",
    "src/styles.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/ssr.ts src/plugins.ts src/ui.ts src/data-grid.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts src/ssr.ts src/plugins.ts src/ui.ts src/data-grid.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "ui:add": "pnpm dlx shadcn@latest add"
  },
  "devDependencies": {
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "8.5.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@flexkit/core": "workspace:*",
    "@flexkit/asset-manager": "workspace:*",
    "@flexkit/desk": "workspace:*",
    "@flexkit/explorer": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
