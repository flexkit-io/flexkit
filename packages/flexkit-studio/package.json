{
  "name": "@flexkit/studio",
  "version": "0.0.4",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexkit-io/flexkit",
    "directory": "packages/flexkit-studio"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ssr": {
      "types": "./dist/ssr.d.ts",
      "import": "./dist/ssr.js"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/plugins.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js"
    },
    "./data-grid": {
      "types": "./dist/data-grid.d.ts",
      "import": "./dist/data-grid.js"
    },
    "./styles.css": "./src/styles.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**",
    "src/styles.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/ssr.ts src/plugins.ts src/ui.ts src/data-grid.ts --format esm --dts",
    "dev": "tsup src/index.ts src/ssr.ts src/plugins.ts src/ui.ts src/data-grid.ts --format esm --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "ui:add": "pnpm dlx shadcn@latest add"
  },
  "devDependencies": {
    "@flexkit/asset-manager": "workspace:*",
    "@flexkit/core": "workspace:*",
    "@flexkit/desk": "workspace:*",
    "@flexkit/explorer": "workspace:*",
    "@types/node": "^20.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^1.0.6",
    "tsconfig": "workspace:*",
    "tsup": "8.5.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
