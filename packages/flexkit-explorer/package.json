{
  "name": "@flexkit/explorer",
  "version": "0.0.7",
  "description": "Flexkit plugin for running and debugging GraphQL queries in a graphical interactive in-browser GraphQL IDE",
  "keywords": [
    "flexkit",
    "studio",
    "react",
    "node",
    "graphql",
    "flexkit-plugin"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "style": "dist/index.css",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexkit-io/flexkit",
    "directory": "packages/flexkit-explorer"
  },
  "exports": {
    ".": "./dist/index.js",
    "./server": "./dist/server/index.js",
    "./tailwind.config": "./tailwind.config.ts",
    "./styles.css": "./dist/index.css"
  },
  "typesVersions": {
    "*": {
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o dist/index.css && tsup src/index.tsx --format esm --dts --external react",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch & tsup src/index.tsx --format esm --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@flexkit/studio": "workspace:*",
    "@types/node": "^20.5.2",
    "@types/ramda": "^0.29.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "3.4.10",
    "tsconfig": "workspace:*",
    "tsup": "8.5.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@graphiql/plugin-history": "0.0.1",
    "@graphiql/react": "0.31.0",
    "@graphiql/toolkit": "0.11.3",
    "graphql-ws": "5.14.2",
    "lucide-react": "0.555.0",
    "ramda": "^0.29.1",
    "swr": "^2.2.4"
  },
  "peerDependencies": {
    "@flexkit/studio": "workspace:*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
