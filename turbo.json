{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["DEBUG"]
    },
    "start": {},
    "clean": {
      "cache": false
    },
    "ui:add": {
      "cache": false
    }
  }
}
